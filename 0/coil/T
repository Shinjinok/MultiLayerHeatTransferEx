FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/core";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 400;

boundaryField
{
    ".*"
    {
       type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
    }
    
    "wedge.*"
    {
        
        type            fixedValue;
        value           $internalField;
    }
}

