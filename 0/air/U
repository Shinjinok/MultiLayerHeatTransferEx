FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (1e-3 0 0);

boundaryField
{

   
    wall_air
    {
        type    noSlip;
    }

    air_to_ring
    {
        type            noSlip;
    }
    air_to_core
    {
        type            noSlip;
    }
}

