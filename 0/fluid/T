FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    outlet_fluid
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    inlet_fluid
    {
        type            fixedValue;
        value           $internalField;
    }
    wall_fluid
    {
        type            zeroGradient;
    }
    fluid_to_case
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
    }
    fluid_to_rotor
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
    }

    }
}