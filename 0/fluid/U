FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 1 0);

boundaryField
{

    outlet_fluid
    {
        //type            pressureInletOutletVelocity;
       // value           $internalField;
       type            zeroGradient;
    }
    
    inlet_fluid
    {
        type            fixedValue;
        value           $internalField;
      //  type            surfaceNormalFixedValue;
      //  refValue        uniform 10; // [m/s]

    }

    wall_fluid
    {
         type    slip;
    }

    "fluid_to.*"
    {
        type    noSlip;
    }
    
    guid_fluid
    {
        type            noSlip;
    }    

}

