FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 0 1);

boundaryField
{

    outlet_fluid
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
    inlet_fluid
    {
        type            fixedValue;
        value           $internalField;
    }
    wall_fluid
    {
         type    noSlip;
    }
    fluid_to_case
    {
        type    noSlip;
    }
    fluid_to_rotor
    {
         type    noSlip;
    }


}

// ************************************************************************* //
