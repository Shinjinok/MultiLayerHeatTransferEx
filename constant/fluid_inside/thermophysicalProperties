FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/fluid_inside";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    // air

    specie
    {
        nMoles          1;
        molWeight       28.96;//g
    }
    equationOfState
    {
        rho             1.21;// kg/m3
    }
    thermodynamics
    {
        Cp              1010;
        Hf              2.544e+06;
    }
    transport
    {
        mu              1.82e-05;
        Pr              0.713;
    }
}

