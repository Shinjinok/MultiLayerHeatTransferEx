FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

temperatureLimit
{
    type limitTemperature;
    actice true;

    limitTemperatureCoeffs
    {
        selectionMode all;
        min 270;
        max 350;
    }
}

